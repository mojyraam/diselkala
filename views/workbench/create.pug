extends ../layout
block append script
block content
    .container
        form(method='POST' action='/workbench/save')
            .containerinfo
                .row.mt-2.mb-2
                    .col-md-3.pr-5
                        label(for="name") نام:
                    .col-md-3.pl-5
                        input#name.form-control(type="text" name="name" pattern=".{6,}" required="is required" oninvalid="setCustomValidity('at least 6 characters is required')" oninput="setCustomValidity('')" maxlength="10")
                    .col-md-3.pr-5
                        label(for="price") قیمت:
                    .col-md-3.pl-5
                        input#price.form-control(type="text" name="price" required="is required" oninvalid="this.setCustomValidity('Please Enter Product Description')" oninput="setCustomValidity('')" maxlength="50")
                .row.mt-2.mb-2
                    .col-md-3.pr-5
                        label(for="branch") شاخه:
                    .col-md-9.pl-5
                        select#branch.form-control(name="branch" required)
                            each item in branch
                                option(value=item.code) #{item.name}
                .row.mt-2.mb-2
                    .col-md-3.pr-5
                        label(for="description") توضیحات:
                    .col-md-9.pl-5
                        textarea#description.form-control(name="description" required="is required" oninvalid="this.setCustomValidity('Please Enter Product Description')" oninput="setCustomValidity('')" maxlength="2000")
